<%- include ('../partials/header') -%>

<!-- Team Section -->
<div class="w3-container" style="padding:128px 16px" id="team">
  <h3 class="w3-center">Sports Event Reviews</h3>
  <p class="w3-center w3-large">Look at the reviews people left</p>
  <div class="w3-row-padding w3-grayscale" style="margin-top:64px">
  
    <% res1.forEach(function(product) { %>
    <div class="w3-col l3 m6 w3-margin-bottom">
      <div class="w3-card">
        <img src="<%= product.image %>" alt="John" style="width:100%">
        <div class="w3-container">
          <h3><%= product.eventName %></h3>
      
          <p><%= product.review %></p>
          <p class="w3-opacity">Left By: <%= product.username %></p>
          <p><a href="/add" class="w3-button w3-light-grey w3-block"><i class="fa fa-envelope"></i> Leave Your Own</a></p>
        </div>
      </div>
    </div>
    <% }); %>
   








  
  </div>
</div>


